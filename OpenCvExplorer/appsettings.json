{
  "Serilog": {
    "MinimumLevel": "Verbose", // Verbose, Debug, Information, Warning, Error, Fatal
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/OpenCvExplorer.log",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:o} [{Level:u3}, {Properties}] ({MachineName}/Pid:{ProcessId}/Tid:{ThreadId}) {Message}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ]
  }
}